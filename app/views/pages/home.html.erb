<table>
  <tr>
    <th>Users</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
    </tr>
  <% end %>
</table>

<table>
  <tr>
    <th>Cats</th>
  </tr>
  <% @cats.each do |cat| %>
    <tr>
      <td><%= cat.name %></td>
    </tr>
  <% end %>
</table>

<table>
  <tr>
    <th>Todos</th>
  </tr>
  <% @todos.each do |todo| %>
    <tr>
      <td><%= todo.tasks %></td>
    </tr>
  <% end %>
</table>